{
  "name": "agnesmueller-com",
  "version": "0.1.0",
  "description": "Artist website for Agnes Mueller",
  "main": "index.js",
  "keywords": [
    "Agnes",
    "Mueller",
    "Artist",
    "Germany",
    "Tim",
    "Jungmann"
  ],
  "author": "Tim Jungmann",
  "license": "ISC",
  "scripts": {
    "dev": "cross-env NODE_ENV=development npm-run-all -l clean --parallel start:*",
    "start": "cross-env NODE_ENV=development npm-run-all -l clean --parallel start:*",
    "start:webpack": "webpack --watch",
    "start:eleventy": "eleventy --serve",
    "build": "cross-env NODE_ENV=production npm-run-all -l clean build:*",
    "build:webpack": "webpack",
    "build:eleventy": "eleventy",
    "clean": "rimraf _site",
    "format": "prettier --write **/*.{js,scss,yml} !_site",
    "analyze": "npm-run-all build analyze:*",
    "analyze:js": "source-map-explorer _site/**/*.js _site/**/*.js.map"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-img": "^2.0.1",
    "@11ty/eleventy-navigation": "^0.3.3",
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.2",
    "@babel/preset-env": "^7.18.2",
    "@portabletext/to-html": "^1.0.4",
    "@sanity/client": "^3.4.1",
    "axios": "^1.5.0",
    "babel-loader": "^8.2.5",
    "cheerio": "^1.0.0-rc.11",
    "core-js": "^3.22.8",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.0.0",
    "date-fns": "^2.28.0",
    "dotenv": "^16.0.3",
    "eleventy-plugin-error-overlay": "^1.0.0",
    "eleventy-plugin-purgecss": "^0.2.1",
    "fs": "^0.0.1-security",
    "groq": "^2.33.2",
    "html-minifier": "^4.0.0",
    "js-yaml": "^4.1.0",
    "markdown-it": "^13.0.1",
    "markdown-it-anchor": "^8.6.4",
    "markdown-it-attrs": "^4.1.4",
    "markdown-it-bracketed-spans": "^1.0.1",
    "mini-css-extract-plugin": "^2.6.0",
    "npm-run-all": "^4.1.5",
    "outdent": "^0.8.0",
    "path": "^0.12.7",
    "postcss": "^8.4.14",
    "postcss-inline-svg": "^5.0.0",
    "postcss-loader": "^7.0.0",
    "postcss-preset-env": "^7.7.0",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "sass": "^1.52.1",
    "sass-loader": "^13.0.0",
    "source-map-explorer": "^2.5.2",
    "svg-sprite": "^1.5.4",
    "vinyl": "^2.2.1",
    "webpack": "^5.73.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2",
    "webpack-manifest-plugin": "^5.0.0"
  },
  "browserslist": {
    "production": [
      ">1%",
      "not ie >= 0",
      "not ie_mob >= 0",
      "not op_mini all",
      "not op_mob >= 0"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
